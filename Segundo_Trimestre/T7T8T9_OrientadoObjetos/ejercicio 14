public class Estadio {

       private String ciudad;
       private String empresa;
       private Zona zona;

public Estadio (String c, String e, Zona z){

       this.ciudad = c;
       this.empresa = e;
       this.zona = z;
}

public String getCiudad(){
        return this.ciudad;
}

public String getEmpresa(){
        return this.empresa;
}

public zona getZona(){
        return this.zona;
}

public void setCiudad(String c){
         this.ciudad = c;
}

public void setEmpresa(String e){
          this.empresa = e;
}

public void setZona(Zona z){
           this.zona = z;
}

public String toString(){
            return “ciudad: “ + this.ciudad + “empresa: “ + this.empresa + “zona asignada: “ + this.zona;

